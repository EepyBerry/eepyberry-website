{
  "name": "eepyberry-website",
  "version": "1.3.1",
  "description": "EepyBerry's personal website",
  "main": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "run-p type-check prettify \"build-only {@}\" --",
    "preview": "vite preview",
    "preview-host": "vite preview --host",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prettify": "prettier . --write",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EepyBerry/eepyberry-website.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "typescript"
  ],
  "author": "EepyBerry",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/EepyBerry/eepyberry-website/issues"
  },
  "homepage": "https://github.com/EepyBerry/eepyberry-website#readme",
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@unhead/vue": "^1.11.11",
    "iconify-icon": "^2.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vue/tsconfig": "^0.6.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.31.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-compression": "^0.5.1",
    "vue-tsc": "^2.1.10"
  }
}
